{
    "name": "freshvine/expiringmediacache",
    "description": "A simple way to cache media locally and remove it after it expires.",
    "keywords": ["cache", "media", "images", "expire", "video"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Prins",
            "role": "Developer",
            "homepage": "https://paul.build/",
            "email": "paulprins@gmail.com"
        },
        {
            "name": "Fresh Vine",
            "homepage": "https://freshvine.co/"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8.35",
        "lupka/phpunit-compare-images": "^1.0"
    },
    "autoload": {
        "psr-4": {"FreshVine\\ExpiringMediaCache\\": ""},
        "exclude-from-classmap": [
            "/docker/",
            "/test/"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "TestExpiringMediaCache": "test/",
            "InstagramScraper": "test/",
            "ExpiringMediaCacheTest": "test/"
        }
    }
}
